#include <iostream>
using namespace std;

int main() {
  int a;
  int b;
  int c;
  cin >> a;
  cin >> b;
  cin >> c;
  if (a >= c) {
    cout << "Bravo!" << endl;
  } 
  else {
    cout << "Try Again" << endl;
  }

  cin >> a;
  cin >> b;
  cin >> c;
  if (a >= c) {
    cout << "Bravo!";
  } 
  else {
    cout << "Try Again";
  }
  return 0;
}
